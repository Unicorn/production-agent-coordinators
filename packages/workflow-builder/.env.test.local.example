# Performance Test Environment Configuration
# Copy this file to .env.test.local and configure with your values

# Required: Authentication token for API access
SUPABASE_TEST_ACCESS_TOKEN=your_test_access_token_here
SUPABASE_TEST_REFRESH_TOKEN=your_refresh_token_here

# Optional: Override base URL
BASE_URL=http://localhost:3010

# Optional: Test environment
TEST_ENV=local

# Optional: Database connection (if testing against local DB)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/workflow_builder_test

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Optional: Performance test configuration
SKIP_CLEANUP=false
ALLOW_PRODUCTION_CLEANUP=false
