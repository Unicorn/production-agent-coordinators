{
  "name": "@coordinator/agent-package-builder-production",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b && npm run copy-templates",
    "copy-templates": "mkdir -p dist/prompts && cp -r src/prompts/*.hbs dist/prompts/",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cli": "vitest run cli-integration.e2e.test.ts",
    "test:cli:watch": "vitest cli-integration.e2e.test.ts",
    "test:integration": "./scripts/test-cli-integration.sh",
    "test:integration:phase4": "vitest run integration/phase4-e2e.test.ts",
    "test:integration:phase4:watch": "vitest integration/phase4-e2e.test.ts",
    "start:worker": "node dist/worker.js",
    "optimization-dashboard": "tsx src/scripts/optimization-dashboard.ts",
    "optimization-runner": "tsx src/scripts/optimization-runner.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@google/genai": "^1.30.0",
    "@temporalio/activity": "^1.10.0",
    "@temporalio/client": "^1.10.0",
    "@temporalio/worker": "^1.10.0",
    "@temporalio/workflow": "^1.10.0",
    "handlebars": "^4.7.8",
    "jsonrepair": "^3.13.1",
    "simple-git": "^3.30.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/node": "^22.7.5",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  }
}
