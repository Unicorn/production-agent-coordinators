# Production Package Builder Environment Variables
# Copy this to .env and fill in your values

# Required: NPM auth token for publishing packages
NPM_TOKEN=npm_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Required: Path to your tools workspace
WORKSPACE_ROOT=/Users/mattbernier/projects/tools

# Optional: Temporal connection settings (defaults shown)
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=agent-coordinator-queue

# Optional: Dry run mode (true = don't actually publish)
DRY_RUN=false
